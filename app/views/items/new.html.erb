<h2 class="text-center my-4"><strong> Sell Something... </strong></h2>
<div class="container">
  <div class="new-item">
    <%= simple_form_for(@item) do |f| %>
      <%= f.input :item_name %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :photo, as: :file %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
